generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model token {
  address  String @id
  name     String
  symbol   String
  uri      String
}

model ifpsData {
  id          Int      @id @default(autoincrement())
  description String
  image       String
  showName    Boolean
  createdOn   String // https://pump.fun (Texto padr√£o)
  twitter     String
  telegram    String
  website     String
}