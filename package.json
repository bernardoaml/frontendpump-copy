{
  "name": "presale",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "fast": "next dev --turbo",
    "debug": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "reset": "rm -rf ./.next",
    "pkg-reset": "rm -rf ./node_modules && rm -f ./pnpm-lock.yaml && pnpm i",
    "cold-build": "npm run reset && npm run build",
    "cold-dev": "npm run reset && npm run dev",
    "cold-start": "npm run reset && npm run build && npm run start",
    "db:push": "drizzle-kit push:pg --config=./src/configs/drizzle.config.ts",
    "db:seed": "tsx ./src/database/postgres/seeders/initial",
    "db:generate": "drizzle-kit generate:pg --config=./src/configs/drizzle.config.ts",
    "db:drop": "drizzle-kit drop --config=./src/configs/drizzle.config.ts",
    "db:studio": "drizzle-kit studio --config=./src/configs/drizzle.config.ts",
    "db:migrate": "tsx src/database/postgres/migrate.ts",
    "db:reset": "rm -rf ./src/database/postgres/drizzle"
  },
